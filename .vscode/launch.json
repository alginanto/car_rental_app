    "version": "0.2.0",
    "configurations": [
        {
            "name": "car_rental (local)",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define=BASE_URL=http://192.168.1.113:8080"
            ]
        },
                
        {
            "name": "car_rental_app",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define=BASE_URL=https://premium-cars-api.onrender.com"
            ]
        },
        {
            "name": "car_rental_app (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--dart-define=BASE_URL=https://premium-cars-api.onrender.com"
            ]
        },
        {
            "name": "car_rental_app (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--dart-define=BASE_URL=https://premium-cars-api.onrender.com"
            ]
        }
    ]
}